<script type="text/javascript">
$('#ittoku').submit(function(event) {
    // HTMLでの送信をキャンセル
    event.preventDefault();

    // 操作対象のフォーム要素を取得
    var $form = $(this);

    // 送信ボタンを取得
    var $button = $form.find('button');

    // 送信
    $.ajax({
        url: $form.attr('action'),
        type: $form.attr('method'),
        data: $form.serialize(),
<!--
        timeout: 10000,  // 単位はミリ秒

        // 送信前
        beforeSend: function(xhr, settings) {
            // ボタンを無効化し、二重送信を防止
            $button.attr('disabled', true);
        },
        // 応答後
        complete: function(xhr, textStatus) {
            // ボタンを有効化し、再送信を許可
            $button.attr('disabled', false);
        },
-->
        // 通信成功時の処理
        success: function(result, textStatus, xhr) {
            // 入力値を初期化
            $form[0].reset();
           
            $( '#result_space' ).html( result );
            //text('OK');
        },

        // 通信失敗時の処理
        error: function(xhr, textStatus, error) {
            alert('NG...');
        }
    });
});
</script>
<script type="text/javascript">
function entryChange1(){
	radio = document.getElementsByName('time')
	if(radio[0].checked) {
		document.getElementById('timeset').style.display = "none";
	}
	else if(radio[1].checked) {
		document.getElementById('timeset').style.display = "";
	}
}
window.onload = entryChange1;
</script>
<script type="text/javascript">
    if (navigator.geolocation) {
        var latitude;
        var longitude;
        // 現在の位置情報取得を実施
        navigator.geolocation.getCurrentPosition(
        // 位置情報取得成功時
        function (pos) { 
                //lat,lngを変数に入れる
                latitude =pos.coords.latitude;
                longitude = pos.coords.longitude;
                //住所入力フォームを表示しない
                document.getElementById('addressSet').style.display = "none";
                document.getElementById('SelectAddress').style.display = "";

                //地図を表示
                //var output = document.getElementById("result_space");
                //var img = new Image();
                //img.src = "https://maps.googleapis.com/maps/api/staticmap?center=" + latitude + "," + longitude + "&zoom=15&size=300x300&sensor=false";
                //output.appendChild(img);

        },

        // 位置情報取得失敗時
        function (pos) { 

          latitude = "";
          longitude = "";
          window.alert("位置情報が取得できませんでした。");
          //住所入力フォームを表示
          document.getElementById('addressSet').style.display = "";
          document.getElementById('SelectAddress').style.display = "none";
        });
    } else {
        window.alert("本ブラウザでは位置情報を取得できませんでした");
    }

    //いっとくボタンを押したらパラメータにlat,lng情報追加
        function funcBtn() {
          // エレメントを作成
          var lat = document.createElement('input');
          var lng = document.createElement('input');
          // データを設定
          lng.setAttribute('type', 'hidden');
          lng.setAttribute('name', 'lng');
          lng.setAttribute('value', longitude);
          lat.setAttribute('type', 'hidden');
          lat.setAttribute('name', 'lat');
          lat.setAttribute('value', latitude);
          // 要素をパラメータに追加
          document.ittoku.appendChild(lng);
          document.ittoku.appendChild(lat);
        }
</script>
